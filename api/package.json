{
    "name": "@napp/api",
    "version": "1.0.16",
    "description": "Rest full api",
    "types": "lib/index.d.ts",
    "main": "lib/index.js",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "tsc -p tsconfig.prod.json",
        "postbuild": "node build.js",
        "---1": "------------------------------------------------------------------------------------",
        "push:core": "cd dist/core & npm publish --access=public",
        "push:server": "cd dist/client & npm publish --access=public",
        "push:client": "cd dist/server & npm publish --access=public",
        "push:builder": "cd dist/builder & npm publish --access=public",
        "---2": "------------------------------------------------------------------------------------",
        "upversion": "npm version patch",
        "prepush": "npm run upversion & npm run build",
        "push": "npm run push:core & npm run push:server & npm run push:client & npm run push:builder",
        "---3": "------------------------------------------------------------------------------------",
        "test:server": "ts-node -r source-map-support/register -r tsconfig-paths/register test/server/server.ts",
        "test": "mocha -r ts-node/register -r source-map-support/register -r tsconfig-paths/register test/**/*.test.ts",
        "test:builder": "ts-node -r source-map-support/register -r tsconfig-paths/register test/builder.ts",
        "---4": "------------------------------------------------------------------------------------",
        "t4": "ts-node -r source-map-support/register -r tsconfig-paths/register test/test.ts"
    },
    "keywords": [],
    "author": "farcek@gmail.com",
    "license": "ISC",
    "engines": {
        "node": ">=12.14.1"
    },
    "dependencies": {
        "@testdeck/mocha": "^0.1.0",
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.6.4",
        "body-parser": "^1.19.0",
        "cross-fetch": "^3.0.5",
        "express": "^4.17.1",
        "js-convert-case": "^4.1.0",
        "mocha": "^7.2.0",
        "morgan": "^1.10.0",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.19",
        "ts-morph": "^8.1.0",
        "ts-node": "^8.10.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.9.7",
        "openapi-types": "^7.0.1"
    }
}
